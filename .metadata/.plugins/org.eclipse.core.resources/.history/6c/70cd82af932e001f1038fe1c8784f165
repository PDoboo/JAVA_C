package p03.cast;

public class P110var {

	public static void main(String[] args) {
		long var1 =2L;
		float var2 = 1.8f;
		
		double doublevar3 =2.5;
		int intValue =(int)doublevar3;
		int result2 = (int) (var2+intValue);
		System.out.println(result2);
		
		String var4 = "3.9";
		//2+1.8+2.5+3.9=
		double var5 = Double.parseDouble(var4);
		System.out.println(var5);
		
		int result =(int) (var1+var2+result2+var5);
				System.out.println(result);

	}

}
