package p03.cast;

public class P97CastingEx {

	public static void main(String[] args) {
		//강제타입변환:큰 타입을 강제로 작게 나눠 저장함
		int intValue =44032;
		char charValue =(char)intValue;
		System.out.println(charValue);
       
		long longValue = 500; //long이 int 보다 큼
		intValue = (int) longValue;//(int)강제로 나눔
		System.out.println(intValue);
		
		double doubleValue =3.14;//double이 int보다 큼
		intValue =(int) doubleValue;//(int)강제로 나눔
		System.out.println(intValue);
	}

}
