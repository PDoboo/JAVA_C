package P124;

public class P132{
	public static void main(String[] agrs) {
		int x = -100;
		int result1 = +x;
		int result2 = -x;// 부호연산자는 변수의 부호를 유지하거나 변경.
		//+연산자는 변수값의 부호를 유지, -연산자는 변수값의 부호를 +로 바꿈 -와-만나면 +된다
		System.out.println("result1="+result1);// -100
		System.out.println("result2="+result2);// 100
		
		byte b = 100;
		int result3 = -b;
		System.out.println("result3="+result3);
	}
}