package p03.cast;

public class P110var {

	public static void main(String[] args) {
		long var1 =2L;
		float var2 = 1.8f;
		double var3 =2.5;
		String var4 = "3.9";
		//2+1.8+2.5+3.9=
		double var4 = Double.parseDouble(var4);
		System.out.println(var4);
		
//		int result =(var1+var2+var3+var4);
//				System.out.println(result);

	}

}
