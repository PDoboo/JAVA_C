package p02.primitive_Type;

public class P91BooleanEx {
 public static void main(String[] args) {
	 boolean stop = true; //stop변수가 true값을 가지고 있기 때문에 if 블록이 실행되어 "중지합니다" 출력
	 if(stop) { 
		 System.out.println("중지합니다");
	 } else { //만약 stop변수가 false값을 가지고 있다면 "시작합니다" 출력
		 System.out.println("시작합니다");
	 }
 }
}
