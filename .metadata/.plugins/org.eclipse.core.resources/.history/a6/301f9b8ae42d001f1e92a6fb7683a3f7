package p03.cast;

public class P100LongOperationEx {

	int a=10;
	public static void main(String[] args) {
		byte b1 =10;
		int i1 =100;
		long l1 =1000l;//long타입 자동형변환(promotion)
		long l2 =b1+i1+l1;//가장큰타입이 기준이되어 연산
		System.out.println(l2);
        
		String str =3+7+"77";
		System.out.println(str); //숫자 1077출력
		String str2 ="77"+3+7;
		System.out.println(str2);//문자 7737출력 "77"에 3과 7이 붙음
		
		//문자열을 숫자로 parseInt()
		String str3 = "200";
		int ins =Integer.parseInt(str3);
		
	}
    public void main2() {
		

	}


}
