package p01.variable;

public class P73VariableEx {

	static int a ; // 전역변수는 초기화 필요없음.(기본값 0)
	//static 붙은것 - RAM에있기때문에 곧바로 사용
	short c = 120;
	public static void main(String[] args) {
		// 변수 value 선언
		double dos = 10.0; // 지역변수는 반드시 값이있어야한다.(초기화)
        int value = 0;
        System.out.println(value);
        int result = value+10;
        System.out.println(result);
        System.out.println(a);
        System.out.println(P73VariableEx.a);
              
        
        System.out.println(c);
        System.out.println(P73VariableEx.a);
	}

}
